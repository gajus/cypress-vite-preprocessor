{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/cypress-vite-preprocessor/**/*"
    ],
    "ignoredByWatcher": [
      "test/fixtures/**/*"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "description": "Cypress preprocessor for bundling JavaScript via Vite.",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@types/node": "^17.0.22",
    "@types/sinon": "^10.0.11",
    "ava": "^4.1.0",
    "eslint": "^8.11.0",
    "eslint-config-canonical": "^33.0.1",
    "husky": "^7.0.2",
    "semantic-release": "^19.0.2",
    "sinon": "^13.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "cypress",
    "vite"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/src/index.js",
  "name": "cypress-vite-preprocessor",
  "peerDependencies": {
    "cypress": "^10.0.0",
    "rollup": "^2.77.0",
    "vite": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/cypress-vite-preprocessor"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src ./test && tsc --noEmit",
    "test": "ava --serial --verbose"
  },
  "version": "1.0.0"
}
